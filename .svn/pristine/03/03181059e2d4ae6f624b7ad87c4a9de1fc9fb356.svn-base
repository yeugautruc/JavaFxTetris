package de.proglabor.aufgabe4;

import javafx.event.ActionEvent;
import javafx.event.EventHandler;
import javafx.scene.control.Button;
import javafx.scene.text.Font;

public class ButtonTetris extends Button{
	public ButtonTetris(String name,int xSize,int ySize,int posX,int posY,Font font, EventHandler<ActionEvent> e) {
			this.setFont(font);
			this.setText(name);
			this.setPrefSize(xSize, ySize);
			this.setLayoutX(posX);
			this.setLayoutY(posY);
			this.setOnAction(e);
			
	}
}
